{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": "..",
    "paths": {
      "@astql/core": [
        "core/src/",
      ],
      "@astql/core/*": [
        "core/src/*",
      ],
      "@astql/query": [
        "query/src/",
      ],
      "@astql/query/*": [
        "query/src/*",
      ],
      "@astql/languages": [
        "languages/src/",
      ],
      "@astql/languages/*": [
        "languages/src/*",
      ],
      "@astql/parser": [
        "parser/src/",
      ],
      "@astql/parser/*": [
        "parser/src/*",
      ],
      "@astql/traverse": [
        "traverse/src/",
      ],
      "@astql/traverse/*": [
        "traverse/src/*",
      ]
    },
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"]
}
